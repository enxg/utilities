{
  "extends": "./without-decorators.json",
  "compilerOptions": {
    "jsx": "preserve",
    "moduleResolution": "Node",
    "allowJs": true,
    "baseUrl": ".",
    "noEmit": true,
    "types": [
      "node"
    ],
    "paths": {
      "~": [
        "."
      ],
      "~/*": [
        "./*"
      ],
      "@": [
        "."
      ],
      "@/*": [
        "./*"
      ],
      "~~": [
        "."
      ],
      "~~/*": [
        "./*"
      ],
      "@@": [
        "."
      ],
      "@@/*": [
        "./*"
      ],
      "assets": [
        "./assets"
      ],
      "public": [
        "./public"
      ],
      "public/*": [
        "./public/*"
      ],
      "#app": [
        "./node_modules/nuxt/dist/app"
      ],
      "#app/*": [
        "./node_modules/nuxt/dist/app/*"
      ],
      "vue-demi": [
        "./node_modules/nuxt/dist/app/compat/vue-demi"
      ],
      "#vue-router": [
        "./.nuxt/vue-router"
      ],
      "#imports": [
        "./.nuxt/imports"
      ],
      "#build": [
        "./.nuxt"
      ],
      "#build/*": [
        "./.nuxt/*"
      ],
      "#components": [
        "./.nuxt/components"
      ]
    }
  },
  "include": [
    "./.nuxt/nuxt.d.ts",
    "./**/*"
  ],
  "exclude": [
    "./dist",
    "./.output"
  ]
}
